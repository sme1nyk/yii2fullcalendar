{
	"name": "sme1nyk/yii2fullcalendar",
	"description": "Yii2 fullcalendar Widgets",
	"keywords": ["yii2", "fullcalendar","widget"],
	"homepage": "",
	"type": "yii2-extension",
	"license": "MIT",
	"authors": [
		{
			"name": "Stanislav Melnyk",
			"email": "melomen.one@gmail.com",
			"homepage": "",
			"role": "Developer"
		}
	],
	"support": {
		"source": "https://github.com/sme1nyk/yii2fullcalendar"
	},
	"minimum-stability": "stable",
	"require": {
		"yiisoft/yii2" : "*",
		"yiisoft/yii2-jui" : "*",
		"bower-asset/fullcalendar" : "3.10.2",
		"bower-asset/fullcalendar-scheduler" : "1.10.1"
	},
	"require-dev": {
		"codeception/codeception": "*"
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"repositories": {
				"bower-asset/eve": {
					"type": "bower-vcs",
					"url": "https://github.com/adobe-webplatform/eve.git"
				}
			}
		}
	},
	"conflict": {
		"bower-asset/fullcalendar": "3.10.2"
	},
	"autoload": {
		"psr-4": { "yii2fullcalendar\\": "" }
	}
}
